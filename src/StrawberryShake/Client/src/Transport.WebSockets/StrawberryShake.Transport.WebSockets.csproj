<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>StrawberryShake.Transport.WebSockets</PackageId>
    <AssemblyName>StrawberryShake.Transport.WebSockets</AssemblyName>
    <RootNamespace>StrawberryShake.Transport.WebSockets</RootNamespace>
    <Nullable>enable</Nullable>
    <Description>GraphQL over WebSocket transport implementation for StrawberryShake GraphQL clients.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="$(MicrosoftExtensionsDIVersion)" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="$(MicrosoftExtensionsOptionsVersion)" />
    <PackageReference Include="System.Threading.Channels" Version="$(SystemThreadingChannelsVersion)" />
    <PackageReference Include="System.IO.Pipelines" Version="$(SystemIOPipelinesVersion)" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Core\StrawberryShake.Core.csproj" />
    <ProjectReference Include="..\Resources\StrawberryShake.Resources.csproj" />
  </ItemGroup>

</Project>
